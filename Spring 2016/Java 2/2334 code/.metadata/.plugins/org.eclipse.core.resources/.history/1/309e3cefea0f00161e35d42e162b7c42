
import java.util.ArrayList;
import java.util.List;

public class Series extends Media {
	private Integer endYear;
	private List<Media> episodes;

	public Series(String title, String releaseYear, String yearRange) {
		super(title, Integer.valueOf(releaseYear));
		endYear = Integer.valueOf(yearRange);
	}

	public void addEpisode(Episode obj) {
		episodes.add(obj);
	}

	public Integer getEndYear() {
		return this.endYear;
	}

	public List<Media> getEpisodes() {
		return this.episodes;
	}

	@Override
	public String getMediaType() {
		return "SERIES";
	}

	public String toString() {
		String temp = "SERIES: " + super.getTitle() + " (" + super.getReleaseYear() + "-" + this.endYear;
		return temp;
	}
}